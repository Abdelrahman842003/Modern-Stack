{
    "preset": "laravel",
    "rules": {
        "simplified_null_return": true,
        "braces": false,
        "new_with_braces": {
            "anonymous_class": true,
            "named_class": true
        },
        "no_unused_imports": true,
        "ordered_imports": {
            "sort_algorithm": "alpha"
        },
        "not_operator_with_successor_space": true,
        "concat_space": {
            "spacing": "one"
        }
    }
}
